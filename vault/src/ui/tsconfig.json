  "compilerOptions": {
    "target": "es5",
    "module": "es6",
    "lib": ["es6", "dom"],
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": false,
    "noImplicitAny": false,
    "typeRoots": [
      "./node_modules/@types"
    ],
    "outDir": "../.tmp"
  }
